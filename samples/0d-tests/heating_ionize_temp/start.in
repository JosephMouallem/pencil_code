!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
! Initialisation parameters
!
&init_pars
  cvsid='$Id$'
  unit_length=1e8, unit_velocity=1e5, unit_density=1e-6, unit_temperature=1e0
  Lxyz=1e0, xyz0=0.
  lwrite_aux=T
/
&eos_init_pars
/
&hydro_init_pars
/
&density_init_pars
  initlnrho='const_lnrho', lnrho_const=0.0
/
&entropy_init_pars
  initlnTT='const_lnTT', lnTT_const=4.26750
  ! xHe=.1 ! [wd 23-Jan-2007:] Commented out as there is no xHe in
           !                   temperature_ionization.f90 
/
