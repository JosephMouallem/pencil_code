!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
! Initialisation parameters
!
&init_pars
  cvsid='$Id$'
  unit_length=1e8, unit_velocity=1e5, unit_density=1e-6, unit_temperature=1e0
  Lxyz=1e0, xyz0=0.
  lwrite_aux=T
/
&eos_init_pars
  xHe=.1
/
&hydro_init_pars
/
&density_init_pars
  initlnrho='const_lnrho', lnrho_const=-0.0
/
&entropy_init_pars
  initss='const_ss', ss_const=.1
/
