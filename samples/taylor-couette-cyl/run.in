!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
!  run parameters for kinematic dynamo run with Glen Roberts flow
!
&run_pars
  cvsid='$Id$',
  nt=101, it1=10, cdt=.4, cdtv=.25, isave=10, itorder=3, 
  dsnap=50, dvid=0.5
  lpencil_check=T
/
&eos_run_pars
/
&hydro_run_pars
  lalways_use_gij_etc=T
/
&density_run_pars
/
&viscosity_run_pars
  nu=1e-2
/
