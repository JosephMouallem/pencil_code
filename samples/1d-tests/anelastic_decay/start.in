!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
! Initialisation parameters for the anelastic problem
!
&init_pars
  cvsid='$Id$'
  lwrite_aux=T
/
&eos_init_pars
  gamma=1.
/
&hydro_init_pars
  inituu='sinwave-ux-kx',ampluu=.01
/
&density_init_pars
  initlnrho='-ln(1+u2/2cs02)'
/
&poisson_init_pars
/
