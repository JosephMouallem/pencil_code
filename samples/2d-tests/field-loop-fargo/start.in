!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
! Initialisation parameters for shocktube problem
!
&init_pars
  cvsid='$Id: start.in,v 1.1 2008-11-30 03:16:25 wlyra Exp $'
  xyz0  = 1.,-0.5,-0.1 ! first corner of box
  xyz1  = 2., 0.5, 0.1 ! last corner of box
  lwrite_ic=T, lwrite_aux=T
  coord_system='cylindric'
  lperi=F,T,T
  bcx='f','f','f','f','f','f','f'
  lfargo_advection=T
/
&initial_condition_pars
 amplaa=1e-3
/
&eos_init_pars
  rho0=1.0, 
  cs0=0.01
  gamma=1.0
/
&hydro_init_pars
  inituu='trilinear-y'
  ampl_ux=1.0 !333333
  ampl_uy=0
  ampl_uz=0
/
&density_init_pars
  ldensity_nolog=T
/
&magnetic_init_pars
/
&grav_init_pars
  ipotential='varying-q'
  qgshear=0.0
  g0=1.
/
